{
	"$schema": "http://json.schemastore.org/tsconfig",
	"extends": "@strapi/typescript-utils/tsconfigs/server",
	"compilerOptions": {
		"outDir": "dist",
		"declaration": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"noImplicitThis": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "**/__tests__/**"]
}
